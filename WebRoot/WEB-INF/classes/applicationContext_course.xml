	<bean id="studyCourseManage" class="com.hys.exam.service.local.impl.StudyCourseManageImpl"></bean>
	
	<bean id="studyCourseFacade" class="com.hys.exam.sessionfacade.local.impl.StudyCourseFacadeImpl"></bean>

