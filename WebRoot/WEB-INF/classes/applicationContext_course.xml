		<property name="jdbcTemplate" ref="examJdbcTemplate"/>
	</bean>
	
	<bean id="systemSiteManage" class="com.hys.exam.service.local.impl.SystemSiteManageImpl">
		<property name="systemSiteDAO" ref="systemSiteDAO"/>
